<Window x:Class="TechnetKeyReaderWpf.mainReader"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Technet Key Reader" Height="480" Width="320" Loaded="loadData">
    <Grid Background="#FF202020">
        <Label Content="My Keys" Height="40" Name="keysLbl" VerticalAlignment="Top" Foreground="White" Margin="10,0,10,0" FontFamily="Segoe UI" FontSize="20" />
        <ListBox Name="prodBox" Margin="0,40,0,0" Background="{x:Null}" Foreground="White" ItemContainerStyle="{DynamicResource productItem}" BorderThickness="0">
            <ListBox.Resources>
                <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="Black" />
                <SolidColorBrush x:Key="{x:Static SystemColors.ControlBrushKey}" Color="Black" />
            </ListBox.Resources>
        </ListBox>
    </Grid>
</Window>
